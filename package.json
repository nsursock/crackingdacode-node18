{
  "name": "tea-stack",
  "version": "1.0.0",
  "description": "A starter template for the TEA stack.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all clean -p build:* && grunt",
    "build:11ty": "eleventy --quiet",
    "build:vite": "vite build",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "dev:11ty": "eleventy --quiet --serve --port=$PORT",
    "dev:vite": "vite build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattwaler/tea-stack.git"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore"
  },
  "keywords": [
    "tailwind",
    "eleventy",
    "alpine"
  ],
  "author": "Matt Waler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattwaler/tea-stack/issues"
  },
  "homepage": "https://github.com/mattwaler/tea-stack#readme",
  "dependencies": {
    "@alpinejs/intersect": "^3.13.0",
    "@supabase/supabase-js": "^2.2.3",
    "@vercel/analytics": "^1.1.1",
    "alpinejs": "^3.7.0",
    "argon2": "^0.28.7",
    "child_process": "^1.0.2",
    "date-fns": "^2.27.0",
    "dotenv": "^16.3.1",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.14.1",
    "stripe": "^8.194.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.14",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "gray-matter": "^4.0.3",
    "grunt": "^1.6.1",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-critical": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "pagefind": "^1.0.3",
    "postcss-import": "^15.0.0",
    "tailwindcss": "^3.1.6",
    "tailwindcss-debug-screens": "^2.2.1",
    "vite": "^4.3.9"
  }
}
